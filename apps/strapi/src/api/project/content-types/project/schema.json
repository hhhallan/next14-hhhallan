{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "github": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "technos": {
      "type": "json"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    }
  }
}
